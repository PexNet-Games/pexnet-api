# Environment
NODE_ENV=development

# Frontend URLs
FRONTEND_URL_DEV=http://localhost:3001
FRONTEND_URL_PROD=https://yourfrontend.com

# MongoDB
MONGO_URI_DEV=mongodb://localhost:27017/pexnet-dev
MONGO_URI_PROD=mongodb://localhost:27017/pexnet-prod

# Server
PORT=3000

# Discord OAuth2
DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_CLIENT_SECRET=your_discord_client_secret
DISCORD_CALLBACK_URL=http://localhost:3000/api/auth/discord/callback

# Discord Bot API
DISCORD_BOT_API_URL_DEV=http://localhost:3001
DISCORD_BOT_API_URL_PROD=http://localhost:3001  
DISCORD_BOT_API_PORT=3001
DISCORD_BOT_AUTH_TOKEN=00000000-0000-0000-0000-000000000000

# Session
SESSION_SECRET=your-super-secret-session-key-change-this-in-production
